{
    "project_name": "My AI Agent",
    "project_slug": "{{ cookiecutter.project_name.lower().replace(' ', '-').replace('_', '-') }}",
    "agent_name": "{{ cookiecutter.project_name.lower().replace(' ', '_').replace('-', '_') }}",
    "service_name": "{{ cookiecutter.project_name.lower().replace(' ', '_').replace('-', '_').replace('_agent', '') }}",
    "service_class_name": "{{ cookiecutter.project_name.replace(' ', '').replace('-', '').replace('Agent', '').replace('agent', '') }}ApplicationService",
    "agent_builder_name": "{% if 'agent' in cookiecutter.agent_name %}{{ cookiecutter.agent_name }}_builder{% else %}{{ cookiecutter.agent_name }}_agent_builder{% endif %}",
    "agent_builder_class_name": "{{ cookiecutter.project_name.replace(' ', '').replace('-', '').replace('_', '') }}Builder",
    "first_node_name": "your_node_name",
    "description": "A specialized AI micro-agent for specific tasks",
    "author_name": "Your Name",
    "python_version": "3.12",
    "use_langsmith": ["yes", "no"],
    "openai_model": "gpt-4o-mini",
    "_templates_suffix": "",
    "_copy_without_render": [
        "*.pyc",
        "*.pyo",
        "*.pyd",
        "__pycache__",
        ".git",
        ".tox",
        ".coverage",
        ".coverage.*",
        ".cache"
    ]
} 